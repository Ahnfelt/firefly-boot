type Point(x: Int, y: Int)

type Bool {
    False
    True
}

type Option[T] {
    None
    Some(value: T)
}

Option_default(option: Option[T], value: T): T {
    | Some(x), _ => x
    | None, x => x
}


type TypeContext(
    mutable nextTypeVariable: Int
)

TypeContext_fresh(context: TypeContext): Int {
    let number = context.nextTypeVariable
    context.nextTypeVariable += 1
    number
}
