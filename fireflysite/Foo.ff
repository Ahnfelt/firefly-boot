nodeMain(system: NodeSystem) {
    let path = system.arguments().{
        | [file] => system.path(file)
        | _ => 
            system.writeErrorLine("Missing file name")
            system.exit(1)
    }
    
    if(!path.exists()) {
        system.exit(1)
    } 

    let directory = if(path.isDirectory()) {path} else {path.parent().grab()}
    

}
